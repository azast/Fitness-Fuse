<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/fitness_fuse2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/fitness_fuse2"
        android:supportsRtl="true"
        android:theme="@style/Theme.CompSciIA"
        tools:targetApi="31">
        <activity
            android:name=".ui.RecordActivity"
            android:exported="false" />
        <activity
            android:name=".ui.AddWorkoutActivity"
            android:exported="false" />
        <activity
            android:name=".ui.WorkoutHistoryActivity"
            android:exported="false" />
        <activity
            android:name=".ui.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>